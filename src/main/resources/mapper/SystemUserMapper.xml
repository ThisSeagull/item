<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cq.start.mapper.SystemUserMapper">
    <select id="selectByIdIgnoreStatus" parameterType="long" resultType="SystemUser">
        select * from
        SYSTEM_USER
        where id=#{id}
    </select>

</mapper>
